[strict]
unsafe-getters-setters
untyped-import
untyped-type-import
nonstrict-import
unclear-type
sketchy-null

[lints]
all=error

[ignore]
.*/node_modules/*
<PROJECT_ROOT>\/node_modules\/graphviz\/.*
.*/node_modules/*

[include]

[libs]

[options]
emoji=true
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowExpected
